{
	"manifest_version": 2,

	"name": "Incredule",
	"description": "Take your SIS schedule, and put it in Google Calendar.",
	"version": "1.0",

	"permissions": [
		"tabs"
	],

	"background": {
		"scripts": ["incredule.js"]
	},

	"browser_action": {
		"name": "Incredule",
		"default_icon": "icon.png"
	}
}